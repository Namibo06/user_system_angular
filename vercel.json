{
  "version": 2,
  "builds": [
      {
          "src": "../../api-java",
          "use": "@vercel/java"
      }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "/user-system-angular.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
  ]
}
